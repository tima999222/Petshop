<Window x:Class="Petshop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Icon="/logo.png"
        xmlns:local="clr-namespace:Petshop"
        Background="#ffb960"
        mc:Ignorable="d"
        Title="Зоомагазин" Height="720" Width="1200">
    <Window.Resources>
        <BitmapImage x:Key="DefaultImage" UriSource=""></BitmapImage>
    </Window.Resources>
    
    <Grid>
        <Frame x:Name="Frame" Source="ProductList.xaml" NavigationUIVisibility="Hidden"/>

        <Button x:Name="Exit_Button" HorizontalAlignment="Right" VerticalAlignment="Top" Width="300" Click="Exit_Button_Click">Выйти</Button>
    </Grid>
</Window>
